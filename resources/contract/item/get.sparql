PREFIX org: <http://www.w3.org/ns/org#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX tbfy: <http://data.tbfy.eu/ontology/ocds/>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX schema: <http://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
SELECT (?id as ?oid) ?itm ?enq ?cl1 ?cl2
WHERE {
	?id a tbfy:Contract .
  ?id tbfy:hasContractedItem ?itm .
	OPTIONAL { ?itm tbfy:hasRelatedEnquiry ?enq } .
	OPTIONAL { ?itm tbfy:classificatonScheme ?cl1 } .
  OPTIONAL { ?itm tbfy:hasClassificaton ?cl2 } .
}
