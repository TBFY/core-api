PREFIX org: <http://www.w3.org/ns/org#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX tbfy: <https://theybuyforyou.eu/ontology/ocds/>
PREFIX dcterms: <http://purl.org/dc/terms/>
SELECT DISTINCT (?sid as ?suri) ?s ?tit ?des ?dat ?stat ?val ?cur ?valr ?acp
WHERE {
    ?s tbfy:awardId ?sid .
  	OPTIONAL { ?s dcterms:title ?tit } .
  	OPTIONAL { ?s dcterms:description ?des } .
  	OPTIONAL { ?s tbfy:awardDate ?dat } .
  	OPTIONAL { ?s tbfy:awardStatus ?stat } .
  	OPTIONAL { ?s tbfy:hasAwardValue ?valr } .
  	OPTIONAL { ?valr tbfy:ValueAmount ?val } .
  	OPTIONAL { ?valr tbfy:ValueCurrency ?cur } .
  	OPTIONAL { ?s tbfy:hasAwardPeriod ?acp } .
#  	OPTIONAL { ?acp tbfy:periodStartDate ?acps } .
}
