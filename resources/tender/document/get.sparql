PREFIX ocds: <http://data.tbfy.eu/ontology/ocds#>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX schema: <http://schema.org/>

SELECT ?qdocumentid ?qdocumenttype ?qdocumentlanguage ?qdocumenturl
WHERE {
	?id ocds:hasDocument ?hdoc .
	OPTIONAL { ?hdoc ocds:documentId ?qdocumentid ;
						       ocds:documentType ?qdocumenttype ;
						       dct:language ?qdocumentlanguage ;
						       schema:URL ?qdocumenturl } .
}
