PREFIX ocds: <http://data.tbfy.eu/ontology/ocds#>
PREFIX dct: <http://purl.org/dc/terms/>

SELECT ?s ?stat ?tit ?des
WHERE {
	?s a ocds:Tender .
  ?s ocds:tenderStatus ?stat .
  OPTIONAL { ?s ocds:tenderId ?tenid } .
  OPTIONAL { ?s dct:title ?tit } .
	OPTIONAL { ?s dct:description ?des } .
  FILTER ( regex(?stat, ?status, "i") || !isLiteral(?status) ) .
  FILTER ( regex(?tit, ?title, "i") || !isLiteral(?title) ) .
  FILTER ( regex(?des, ?description, "i") || !isLiteral(?description) ) .
}
