PREFIX org: <http://www.w3.org/ns/org#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX tbfy: <https://theybuyforyou.eu/ontology/ocds/>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX schema: <http://schema.org/>
SELECT DISTINCT (?id as ?oid) ?isf ?dat ?tit ?doc ?status ?period ?value ?cperiod ?item
WHERE {
	?id a tbfy:ContractingProcess .
	OPTIONAL { ?id dcterms:title ?tit } .
  ?id tbfy:hasAward ?isf .
  ?isf a tbfy:Award .
  ?isf tbfy:awardDate ?dat .
  OPTIONAL {?isf tbfy:hasDocument ?doc } .
  OPTIONAL {?isf tbfy:awardStatus ?status} .
  OPTIONAL {?isf tbfy:hasAwardPeriod ?period} .
  OPTIONAL {?isf tbfy:hasAwardValue ?value} .
  OPTIONAL {?isf tbfy:hasAwardedContractPeriod ?cperiod }
  OPTIONAL {?isf tbfy:hasAwardedItem ?item }
}
